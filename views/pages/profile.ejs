        <div class="row">
            <!-- sidebar -->
            <div class="column col-sm-3" id="sidebar">
                <a class="logo" href="#"><img style="width:150px;height:150px;" src="data:<%=image_type%>;base64,<%= image %>" class="profileimg"></a>
            <div class="sidebar-prop">
                <strong><h1 id="profilename" style="text-transform: capitalize"><%= name %></h1></strong>
                <div class="alert alert-warning" role="alert" id="reputation"><h4 id="repuword"><strong><%= like_count*5 %></strong>&nbsp<small> REPUTATION</small></h4></div>
            </div>
            <div class="badges col-sm-3">
							    <div class="badgeitems" ><div class="color-box " style="background-color: #FF850A;"></div>
										 <div style="font-size:18px">Badge_name1</div>
									 </div></br></br>
									 <div class="badgeitems" ><div class="color-box " style="background-color: #52DE3F;"></div>
