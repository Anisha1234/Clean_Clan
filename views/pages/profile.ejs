                            User Details
                          </div>
                        </div>
                         <div class="row" style="margin-left:2%">
                              <div class="col-sm-12">
                                <h4>EMAIL : <span><%= email %></span></h4>
                                <h4>User Details : <span></span></h4>

                              </div>
                        </div>
                        <!-- content -->

                        <div class="col-sm-12" id="featured">
                          <div class="page-header text-muted">
                          Recent Works
                          </div>
                        </div>



                        <!--/stories-->
					<% for(i=0;i<posts.length;i++){ post = posts[i] %>
                        <div class="row">
                          <div class="col-sm-12">
                            <h3><%= post.heading %></h3>
                            <div class="col-md-6 col-sm-12">
                            <a title="Image 1">
                                <img class="thumbnail img-responsive" id="image-2" src="data:<%=post.image_before_type %>;base64,<%= post.image_before %>">
                            </a>
                            </div>
                            <div class="col-md-6 col-sm-12">
                                <h4><span class="label label-success">solution</span></h4><h4>
